                "backend/src/sales/sales.controller.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/sales/daily": {
                        "get": {
                            "summary": "Get daily sales report"
                        }
                    }
                }
            }
        }
    ]
}